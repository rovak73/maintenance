{
    "routes": [
        {
            "src": "/(.*)",
            "status": 503,
            "dest": "/index.html",
            "headers": {
                "Cache-Control": "no-cache, no-store, must-revalidate",
                "Pragma": "no-cache",
                "Expires": "0",
                "Retry-After": "3600"
            }
        }
    ]
}